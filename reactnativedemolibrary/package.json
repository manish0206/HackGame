{
  "name": "reactnativedemolibrary",
  "version": "0.2.1",
  "description": "React Native demo library",
  "main": "index.js",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petterh/react-europe-demo.git"
  },
  "keywords": [
    "react-native",
    "android",
    "java",
    "objective-c"
  ],
  "author": "Petter Hesselberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/petterh/react-europe-demo/issues"
  },
  "homepage": "https://github.com/petterh/react-europe-demo#readme",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "d3-interpolate": "^1.3.2",
    "matter-js": "^0.14.2",
    "matter-lines": "^0.1.0",
    "react": "^16.5.0",
    "react-easy-swipe": "0.0.18",
    "react-motion": "^0.5.2",
    "react-native": "^0.60.5",
    "react-native-elements": "^1.1.0",
    "react-native-game-engine": "^0.12.1",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-immersive": "^2.0.0",
    "react-native-material-dialog": "^0.7.6",
    "react-native-particles": "0.0.7",
    "react-native-popup-dialog": "^0.18.3",
    "react-native-share": "^2.0.0",
    "react-native-sound": "^0.11.0",
    "react-native-sound-player": "^0.10.0",
    "react-native-svg": "^9.8.4",
    "react-native-swipe-gestures": "^1.0.3",
    "react-native-tcp": "^3.3.1",
    "react-navigation": "^3.11.1",
    "readable-stream": "^1.0.33",
    "stream-browserify": "^1.0.0"
  },
  "devDependencies": {
    "hermes-engine": "^0.1.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.15.1",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "husky": "^3.0.3",
    "lerna": "^3.3.0",
    "lint-staged": "^8.2.1",
    "precommit": "^1.2.2",
    "rn-nodeify": "^10.1.0",
    "xo": "^0.24.0"
  },
  "prettier": {
    "printWidth": 100,
    "useTabs": true,
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "ignore": [
      "**/views/**/*.js",
      "**/public/**/*.js",
      "**/static/**/*.js"
    ],
    "linters": {
      "*.js": [
        "xo --fix --quiet",
        "git add",
        "xo --quiet"
      ]
    }
  },
  "xo": {
    "parser": "babel-eslint",
    "esnext": true,
    "prettier": true,
    "rules": {
      "no-console": 2,
      "node/prefer-global/buffer": [
        "error",
        "always"
      ],
      "no-new": [
        "warn"
      ],
      "require-await": [
        0
      ],
      "unicorn/filename-case": [
        "warn"
      ],
      "unicorn/catch-error-name": [
        "warn"
      ],
      "capitalized-comments": [
        0
      ],
      "no-class-assign": [
        0
      ],
      "new-cap": [
        0
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-arrow-callback": [
        "error",
        {
          "allowNamedFunctions": true
        }
      ],
      "prefer-promise-reject-errors": [
        "warn"
      ],
      "unicorn/prefer-node-append": [
        "warn"
      ],
      "prefer-destructuring": [
        "warn"
      ],
      "complexity": [
        "warn"
      ],
      "max-params": [
        "warn",
        {
          "max": 5
        }
      ],
      "camelcase": [
        "warn"
      ]
    },
    "ignores": [
      "static/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "react-native": {
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "net": "react-native-tcp"
  }
}
